{% extends '::base.html.twig' %}


{% block pageheader %}
    <h1>TermOfUse</h1>
{% endblock %}

{% block body -%}
    <div class="btn-toolbar">
        <div class="btn-group pull-right">
            <a href="{{ path('termofuse') }}" class="btn btn-default">
                <span class="glyphicon glyphicon-arrow-left"></span> Back
            </a>
            <a href="{{ path('termofuse_edit', { 'id': entity.id }) }}" class="btn btn-default">
                <span class='glyphicon glyphicon-edit'></span> Edit
            </a>
            <a href="{{ path('termofuse_history', { 'id': entity.id }) }}" class="btn btn-default">
                <span class='glyphicon glyphicon-time'></span> History
            </a>
            <a href="{{ path('termofuse_delete', { 'id': entity.id }) }}" class="btn btn-default js-confirm" data-confirm="Are you sure you want to delete this TermOfUse?">
                <span class='glyphicon glyphicon-remove'></span> Delete
            </a>
        </div>
    </div>

    <table class="record_properties table table-striped table-bordered table-hover">
        <tbody>
            <tr>
                <th>Weight</th>
                <td>{{ entity.weight }}</td>
            </tr>
            <tr>
                <th>Created</th>
                <td>{{ entity.created|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Keycode</th>
                <td>{{ entity.keyCode }}</td>
            </tr>
            <tr>
                <th>Langcode</th>
                <td>{{ entity.langCode }}</td>
            </tr>
            <tr>
                <th>Content</th>
                <td>{{ entity.content }}</td>
            </tr>
        </tbody>
    </table>
{% endblock %}
