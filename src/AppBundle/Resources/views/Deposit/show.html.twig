{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Deposit</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>File_uuid</th>
                <td>{{ entity.fileuuid }}</td>
            </tr>
            <tr>
                <th>Deposit_uuid</th>
                <td>{{ entity.deposituuid }}</td>
            </tr>
            <tr>
                <th>Received</th>
                <td>{{ entity.received|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Action</th>
                <td>{{ entity.action }}</td>
            </tr>
            <tr>
                <th>Volume</th>
                <td>{{ entity.volume }}</td>
            </tr>
            <tr>
                <th>Issue</th>
                <td>{{ entity.issue }}</td>
            </tr>
            <tr>
                <th>Pubdate</th>
                <td>{{ entity.pubDate|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Checksumtype</th>
                <td>{{ entity.checksumType }}</td>
            </tr>
            <tr>
                <th>Checksumvalue</th>
                <td>{{ entity.checksumValue }}</td>
            </tr>
            <tr>
                <th>Url</th>
                <td>{{ entity.url }}</td>
            </tr>
            <tr>
                <th>Size</th>
                <td>{{ entity.size }}</td>
            </tr>
            <tr>
                <th>State</th>
                <td>{{ entity.state }}</td>
            </tr>
            <tr>
                <th>Outcome</th>
                <td>{{ entity.outcome }}</td>
            </tr>
            <tr>
                <th>Plnstate</th>
                <td>{{ entity.plnState }}</td>
            </tr>
            <tr>
                <th>Depositdate</th>
                <td>{{ entity.depositDate|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Depositreceipt</th>
                <td>{{ entity.depositReceipt }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('deposit') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('deposit_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
