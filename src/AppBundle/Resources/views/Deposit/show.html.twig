{% extends '::base.html.twig' %}

    
{% block pageheader %}
    <h1>Deposit</h1>
{% endblock %}
    
{% block body -%}
        <div class="btn-toolbar">
    <div class="btn-group pull-right">
        <a href="{{ path('deposit') }}" class="btn btn-default">
            <span class="glyphicon glyphicon-arrow-left"></span> Back
        </a>
    </div>
</div>

    <table class="record_properties table table-striped table-bordered table-hover">
        <tbody>
            <tr>
                <th>File_uuid</th>
                <td>{{ entity.fileuuid }}</td>
            </tr>
            <tr>
                <th>File_type</th>
                <td>{{ entity.filetype }}</td>
            </tr>
            <tr>
                <th>Deposit_uuid</th>
                <td>{{ entity.deposituuid }}</td>
            </tr>
            <tr>
                <th>Received</th>
                <td>{{ entity.received|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Action</th>
                <td>{{ entity.action }}</td>
            </tr>
            <tr>
                <th>Volume</th>
                <td>{{ entity.volume }}</td>
            </tr>
            <tr>
                <th>Issue</th>
                <td>{{ entity.issue }}</td>
            </tr>
            <tr>
                <th>Pubdate</th>
                <td>{{ entity.pubDate|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Checksumtype</th>
                <td>{{ entity.checksumType }}</td>
            </tr>
            <tr>
                <th>Checksumvalue</th>
                <td>{{ entity.checksumValue }}</td>
            </tr>
            <tr>
                <th>Url</th>
                <td>{{ entity.url }}</td>
            </tr>
            <tr>
                <th>Size</th>
                <td>{{ entity.size }}</td>
            </tr>
            <tr>
                <th>State</th>
                <td>{{ entity.state }}</td>
            </tr>
            <tr>
                <th>Plnstate</th>
                <td>{{ entity.plnState }}</td>
            </tr>
            <tr>
                <th>Packagesize</th>
                <td>{{ entity.packageSize }}</td>
            </tr>
            <tr>
                <th>Packagepath</th>
                <td>{{ entity.packagePath }}</td>
            </tr>
            <tr>
                <th>Packagechecksumtype</th>
                <td>{{ entity.packageChecksumType }}</td>
            </tr>
            <tr>
                <th>Packagechecksumvalue</th>
                <td>{{ entity.packageChecksumValue }}</td>
            </tr>
            <tr>
                <th>Depositdate</th>
                <td>{{ entity.depositDate|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Depositreceipt</th>
                <td>{{ entity.depositReceipt }}</td>
            </tr>
            <tr>
                <th>Processinglog</th>
                <td>{{ entity.processingLog }}</td>
            </tr>
        </tbody>
    </table>
{% endblock %}
