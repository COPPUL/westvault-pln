{% extends '::base.html.twig' %}

    
{% block pageheader %}
    <h1>Whitelist</h1>
{% endblock %}
    
{% block body -%}
        <div class="btn-toolbar">
    <div class="btn-group pull-right">
        <a href="{{ path('whitelist') }}" class="btn btn-default">
            <span class="glyphicon glyphicon-arrow-left"></span> Back
        </a>
        <a href="{{ path('whitelist_edit', { 'id': entity.id }) }}" class="btn btn-default">
            <span class='glyphicon glyphicon-edit'></span> Edit
        </a>
        <a href="{{ path('whitelist_delete', { 'id': entity.id }) }}" class="btn btn-default js-confirm" data-confirm="Are you sure you want to delete this Whitelist?">
            <span class='glyphicon glyphicon-remove'></span> Delete
        </a>
    </div>
</div>

    <table class="record_properties table table-striped table-bordered table-hover">
        <tbody>
            <tr>
                <th>Uuid</th>
                <td>{{ entity.uuid }}</td>
            </tr>
            <tr>
                <th>Comment</th>
                <td>{{ entity.comment }}</td>
            </tr>
            <tr>
                <th>Created</th>
                <td>{{ entity.created|date('Y-m-d H:i:s') }}</td>
            </tr>
        </tbody>
    </table>
{% endblock %}
